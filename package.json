{
	"name": "pixelspace",
	"description": "Pixelspace is minimalist tool for drawing pixels to an HTML5 Canvas",
	"version": "0.2.0",
	"author": "@jeremyfromearth",
  "scripts": {
    "build": "npx babel src --out-dir lib --presets=es2015,stage-0 --plugins=transform-es2015-modules-amd",
    "watch": "npx babel src --out-dir lib --presets=es2015,stage-0 --plugins=transform-es2015-modules-amd --watch",
    "demos": "open http://localhost:8000/demo; python -m http.server",
    "build-demos": "npx babel demo/src --out-dir demo/js --presets=es2015,stage-0 --plugins=transform-es2015-modules-amd"
  }, 
	"keywords": [
		"canvas",
		"drawing",
		"visualization",
		"pixelpace",
		"pixels",
    "creative-coding",
    "simulation"
	],
	"repository": {
		"type": "git",
		"url": "git@github.com:jeremyfromearth/pixelspace.git"
	},
	"homepage": "https://github.com/jeremyfromearth/pixelspace",
	"bugs": {
		"url": "https://github.com/jeremyfromearth/pixelspace/issues"
	},
	"license": "GPL",
	"devDependencies": {
		"babel-cli": "^6.26.0",
		"babel-core": "^6.26.0",
		"babel-plugin-transform-es2015-modules-amd": "^6.24.1",
		"babel-preset-env": "^1.6.1",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-stage-0": "^6.24.1"
	}
}
